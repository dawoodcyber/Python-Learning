    "import re\n",
    "passs = input('Enter password to check the strength')\n",
    "\n",
    "passRegex = re.compile(r'[a-zA-Z0-9]{8}')\n",
    "mo = passRegex.search(passs)\n",
    "\n",
    "if mo:\n",
    "    print('Password is strong')\n",
    "\n",
    "else:\n",
    "    print('Not the strong one!')"
   ]
